<section id="middle">
    <div id="container">
        <strong> Center Content:</strong>
        <%= render 'index' %>
    </div><!-- #container-->
    <aside id="double_side_right">
        <strong> Side</strong>
        <div class="stacks">
            <div class="inner_stack_left">
                <strong> Stack 0</strong>
                <h3>comments/details</h3>
            </div>
            <div class="inner_stack_right">
                <strong> Stack 1</strong>
                <table>
                    <tr>
                        <th>Societies</th>
                    </tr>
                    <% @events.each do |event| %>
                    <tr>
                        <td><%= link_to event.name, event_societies_path(event)%></td>
                    </tr>
                    <% end %>
                </table>
                <p></p>
            </div>
        </div>
        <div class="stacks">
            <div class="inner_stack_left">
                <strong> Stack 2</strong>

            </div>
            <div class="inner_stack_right">
                <strong> Stack 3</strong>
                <table>
                    <tr>
                        <th>Physics</th>
                    </tr>
                    <% @events.each do |event| %>
                    <tr>
                        <td><%= link_to event.name, event_physics_path(event)%></td>
                    </tr>
                    <% end %>
                </table>
            </div>
    </aside>
</section>