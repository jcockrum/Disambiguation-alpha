<%= simple_form_for(:society) do |f| %>
	<%= f.error_notification %>
	
	<div class="form-inputs">
	    <%= f.input :name %>
	    <%= f.select :economics,  :collection =>  Society.EconTypes%>
	    <%= f.select :social_interaction, :collection =>  Society.S_interaction%>
	    <%= f.select :social_material, :collection =>  Society.S_material%>
	</div>
	
	<div class="form-actions">
	    <%= f.button :submit %>
	</div>
<% end %>
