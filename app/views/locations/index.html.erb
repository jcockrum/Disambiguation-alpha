<section id="middle">
    <div id="container">
        <strong> Center Content:</strong>
        <%= render 'index' %>
    </div><!-- #container-->
    <aside id="double_side_right">
        <strong> Side</strong>
        <div class="stacks">
            <div class="inner_stack_left">
                <strong> Stack 0</strong>
                	<h3>Details</h3>
	                <% if @location %>
						<p>most recent event discription for <%=@location.name%></p>
					<% end %>
            </div>
            <div class="inner_stack_right">
                <strong> Stack 1</strong>
                location => events
                <p></p>
            </div>
        </div>
        <div class="stacks">
            <div class="inner_stack_left">
                <strong> Stack 2</strong>
                location => scenes
            </div>
            <div class="inner_stack_right">
                <strong> Stack 3</strong>
				character => events
            </div>
    </aside><!-- #sideLeft -->
</section><!-- #middle-->