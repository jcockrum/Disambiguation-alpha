<section id="middle">
    <div id="container">
        <strong> Center Content:</strong>
        <%= render 'index' %>
    </div><!-- #container-->
    <aside id="double_side_right">
        <strong> Side</strong>
        <div class="stacks">
            <div class="inner_stack_left">
                <strong> Stack 0</strong>
                	<h3>comments/details</h3>
            </div>
            <div class="inner_stack_right">
                <strong> Stack 1</strong>
	                <table>
						<tr><th>Events</th></tr>
						<% @locations.each do |location| %>
						  <tr><td><%= link_to location.name, location_events_path(location)%></td></tr>
						<% end %>
					</table>
                <p></p>
            </div>
        </div>
        <div class="stacks">
            <div class="inner_stack_left">
                <strong> Stack 2</strong>
                <%= render 'storyarcs/storyarcs_sidestack' %>
            </div>
            <div class="inner_stack_right">
                <strong> Stack 3</strong>
				<%= render 'characters/characters_sidestack' %>
            </div>
    </aside>
</section>